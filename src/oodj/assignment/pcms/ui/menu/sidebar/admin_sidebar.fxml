<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="504.0" prefWidth="152.0" stylesheets="@../../../resources/common.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="assignment.pcms.ui.menu.sidebar.AdminSidebarController">
   <children>
      <JFXButton fx:id="userButton" alignment="TOP_CENTER" contentDisplay="TOP" onAction="#showUser" prefHeight="67.0" prefWidth="230.0" text="Manage Users">
         <graphic>
            <ImageView fitHeight="59.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/icons/users.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="supplierButton" alignment="TOP_CENTER" contentDisplay="TOP" onAction="#showSuppliers" prefHeight="67.0" prefWidth="230.0" text="Manage Suppliers">
         <graphic>
            <ImageView fitHeight="59.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/icons/suppliers.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="productButton" alignment="TOP_CENTER" contentDisplay="TOP" onAction="#showProducts" prefHeight="67.0" prefWidth="230.0" text="Manage Products">
         <graphic>
            <ImageView fx:id="showProducts" fitHeight="59.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/icons/products.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="sessionButton" alignment="TOP_CENTER" contentDisplay="TOP" onAction="#showSession" prefHeight="67.0" prefWidth="230.0" text="Manage Session Logs">
         <graphic>
            <ImageView fitHeight="59.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/icons/logs.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="profileButton" alignment="TOP_CENTER" contentDisplay="TOP" onAction="#showProfile" prefHeight="67.0" prefWidth="230.0" text="Manage Profile">
         <graphic>
            <ImageView fitHeight="59.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/icons/profile.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="logoutButton" alignment="TOP_CENTER" contentDisplay="TOP" onAction="#logout" prefHeight="67.0" prefWidth="230.0" text="Log Out">
         <graphic>
            <ImageView fitHeight="59.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/icons/exit.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
   </children>
</VBox>
