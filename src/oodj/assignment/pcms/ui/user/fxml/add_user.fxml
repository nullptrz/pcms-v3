<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="470.0" prefWidth="356.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="assignment.pcms.ui.user.controllers.AddUserController">
   <children>
      <VBox prefHeight="384.0" prefWidth="356.0">
         <children>
            <JFXTextField fx:id="userid" editable="false" focusColor="#c3d3ff" labelFloat="true">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="name" focusColor="#c3d3ff" labelFloat="true" promptText="Enter Name">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="emailAddress" focusColor="#c3d3ff" labelFloat="true" promptText="Enter Address">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXComboBox fx:id="roleCombo" focusColor="#c3d3ff" prefHeight="26.0" prefWidth="610.0">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
               </VBox.margin>
            </JFXComboBox>
            <JFXTextField fx:id="username" focusColor="#c3d3ff" labelFloat="true" prefHeight="26.0" prefWidth="287.0" promptText="Enter Login Name">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXPasswordField fx:id="password" focusColor="#c3d3ff" labelFloat="true" promptText="Enter Password">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
               </VBox.margin>
            </JFXPasswordField>
         </children>
      </VBox>
      <HBox layoutX="9.0" layoutY="384.0" prefHeight="100.0" prefWidth="336.0">
         <children>
            <JFXButton fx:id="saveButton" onAction="#save" prefHeight="54.0" prefWidth="165.0" text="Save" textFill="WHITE">
               <HBox.margin>
                  <Insets right="10.0" top="25.0" />
               </HBox.margin>
            </JFXButton>
            <JFXButton fx:id="cancelButton" onAction="#cancel" prefHeight="54.0" prefWidth="165.0" text="Cancel" textFill="WHITE">
               <HBox.margin>
                  <Insets top="25.0" />
               </HBox.margin>
            </JFXButton>
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@../css/add_user.css" />
      <URL value="@../../../resources/common.css" />
   </stylesheets>
</AnchorPane>
